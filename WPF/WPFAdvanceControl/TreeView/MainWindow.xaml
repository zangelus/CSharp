<Window x:Class="TreeView.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TreeView"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TreeView>
            <TreeViewItem Header="Animals">
                <TreeViewItem Header="Mamals">
                    <TreeViewItem Header="Dogs">
                        <TreeViewItem Header="Spaniels"></TreeViewItem>
                        <TreeViewItem Header="Working Dogs"></TreeViewItem>
                        <TreeViewItem Header="Retrievers"></TreeViewItem>
                    </TreeViewItem>
                    <TreeViewItem Header="Felinos">
                        <TreeViewItem Header="Cats"></TreeViewItem>
                        <TreeViewItem Header="Tiger"></TreeViewItem>
                        <TreeViewItem Header="Leopard"></TreeViewItem>
                    </TreeViewItem>
                </TreeViewItem>
                <TreeViewItem Header="Marsupials">
                </TreeViewItem>
            </TreeViewItem>
            <TreeViewItem Header="Plants"/>
            <TreeViewItem Header="Others"/>
        </TreeView>
    </Grid>
</Window>
